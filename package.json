{
  "name": "votify",
  "version": "1.0.0",
  "description": "Votify - Allow users to vote on the tracks in a Spotify playlist.",
  "sideEffects": false,
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "NODE_ENV=production webpack --mode production --display-error-details  --config webpack.prod.js",
    "build-dev": "webpack --mode development --display-error-details  --config webpack.dev.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --config webpack.dev.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --watch"
  },
  "author": "Patrik Elfstr√∂m <me@patrikelfstrom.se> (https://patrikelfstrom.se)",
  "license": "MIT",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "debounce-promise": "^3.1.0",
    "express": "^4.16.4",
    "favicons-webpack-plugin": "0.0.9",
    "isomorphic-fetch": "^2.2.1",
    "list.js": "^1.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-cookie": "^3.0.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sqlite3": "^4.0.2",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^7.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrikElfstrom/votify.git"
  },
  "bugs": {
    "url": "https://github.com/PatrikElfstrom/votify/issues"
  }
}
